{
  "hook_key": true,
  "hook_mouse": false,
  "hook_windows": true,
  "hotkey": {
    "win": {
      "type": "start"
    },
    "win+r": {
      "type": "run"
    },
    "win+c": {
      "dir": "",
      "path": "cmd.exe",
      "type": "open"
    },
    "win+d": {
      "type": "desktop"
    },
    "win+e": {
      "param": "::{20D04FE0-3AEA-1069-A2D8-08002B30309D}",
      "path": "explorer.exe",
      "type": "open",

      //此处参数ShowWindow常量.
      "show": 3
    },
    "win+q": {
      "path": "D:\\软件工具\\Everything\\Everything.exe",
      "type": "open"
    },
    "win+z": {
      "path": "C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Enterprise\\Common7\\IDE\\devenv.exe",
      "type": "open"
    },
    "win+l": {
      "type": "null"
    }
  },
  "m_bIs_limit_mouse": false,
  "raw_input": false,

  //hook窗口需要排除的进程
  "hook_windows_filter": [
    {
      "ps": "cefApp.exe"
    },
    {
      "ps": "chrome.exe"
    },
    {
      "ps": "cefMgr.exe"
    }
  ],

  //需要隐藏的窗口
  "hook_windows_hide": [
    {
      "ps": "v2ray-plugin.exe"
    }
  ]
}
