{
    "hook_key": true,
    "hook_mouse": false,
    "hook_windows": true,
    "hotkey": {
        "win": {
            "type": "start"
        },
        "win+c": {
            "dir": "d:\\",
            "path": "cmd.exe",
            "type": "open"
        },
        "win+d": {
            "type": "desktop"
        },
        "win+e": {
            "param": "::{20D04FE0-3AEA-1069-A2D8-08002B30309D}",
            "path": "explorer.exe",
            "type": "open"
        },
        "win+r": {
            "type": "run"
        },
        "win+z": {
            "path": "calc.exe",
            "type": "open"
        }
    },
    "raw_input": false,
    "filter_windows": { //支持对象或数组
        "ps":"devMonitor.exe",
        "title":"",
        "class":""
    },
    "hide_windows":{ //支持对象或数组

    }
}